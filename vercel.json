{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/customers",
      "dest": "/index.js"
    },
    {
      "src": "/api/products",
      "dest": "/index.js"
    },
    {
      "src": "/api/email",
      "dest": "/index.js"
    },
    {
      "src": "/api/users",
      "dest": "/index.js"
    }
  ]
}